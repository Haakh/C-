//print all numbers 'a' from 0 to < n such that they are divisible by x but not by y. 
// Input no. of test cases followed by n,x,y.
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int c,n,x,y;
	cin>>c;
	while(c--){
		cin>>n>>x>>y;
		for(int i =0;i<n;i++){
            if(i%x==0){
                if(!(i%y==0))
                    cout<<i<<" ";
            }
		}
		cout<<endl;
	}
	return 0;
}
